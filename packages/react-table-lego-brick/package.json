    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "nyc": "^14.1.1",
    "power-assert": "^1.6.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "rollup": "^1.27.9",
    "rollup-plugin-commonjs": "^10.1.0",
    "sinon": "^7.5.0",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.2"
