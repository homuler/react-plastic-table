  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf -rf lib/ && rimraf tsconfig.tsbuildinfo",
    "build": "tsc -b",
    "coverage": "nyc mocha 'src/**/*.test.{ts,tsx}' --opts ./mocha.opts",
    "coverage:ci": "nyc mocha 'src/**/*.test.{ts,tsx}' --opts ./mocha.opts --reporter mocha-junit-reporter --reporter-options mochaFile=../../reports/react-table-lego-brick/test-results.xml",
    "coverage:ci:report": "nyc report --reporter=text-lcov > ../../coverage/react-table-lego-brick/lcov.info",
    "coverage:report": "nyc report",
    "precoverage:ci": "mkdir -p ../../reports/react-table-lego-brick",
    "precoverage:ci:report": "mkdir -p ../../coverage/react-table-lego-brick",
    "test": "mocha 'src/**/*.test.{ts,tsx}' --opts ./mocha.opts"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.0",
    "@testing-library/react": "^9.4.0",
    "@types/enzyme": "^3.10.4",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^5.2.7",
    "@types/power-assert": "^1.5.2",
    "@types/react": "^16.9.17",
    "@types/react-is": "^16.7.1",
    "@types/sinon": "^7.5.1",
    "@types/styled-components": "^4.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest": "^24.9.0",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "mocha": "^6.2.2",
    "mocha-junit-reporter": "^1.23.1",
    "nyc": "^15.0.0",
    "power-assert": "^1.6.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "^3.0.0",
    "sinon": "^8.0.1",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "react-draggable": "^4.0.3",
    "react-is": "^16.12.0",
    "styled-components": "^4.4.1"
  }
